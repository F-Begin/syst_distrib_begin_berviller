<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<xs:schema version="1.0" targetNamespace="http://www.polytechNancy.fr" xmlns:tns="http://www.polytechNancy.fr" xmlns:xs="http://www.w3.org/2001/XMLSchema">

  <xs:element name="AjoutEtudiant" type="tns:AjoutEtudiant"/>

  <xs:element name="ExportEtu" type="tns:ExportEtu"/>

  <xs:element name="GetEtuId" type="tns:GetEtuId"/>

  <xs:element name="GetEtuIdResponse" type="tns:GetEtuIdResponse"/>

  <xs:element name="GetEtuInfo" type="tns:GetEtuInfo"/>

  <xs:element name="GetEtuInfoResponse" type="tns:GetEtuInfoResponse"/>

  <xs:element name="ImportEtu" type="tns:ImportEtu"/>

  <xs:element name="InfoAll" type="tns:InfoAll"/>

  <xs:element name="InfoAllResponse" type="tns:InfoAllResponse"/>

  <xs:element name="ModifieEtudiant" type="tns:ModifieEtudiant"/>

  <xs:element name="SuprEtudiant" type="tns:SuprEtudiant"/>

  <xs:complexType name="GetEtuInfo">
    <xs:sequence>
      <xs:element name="id" type="xs:int"/>
    </xs:sequence>
  </xs:complexType>

  <xs:complexType name="GetEtuInfoResponse">
    <xs:sequence>
      <xs:element name="return" type="tns:etudiant" minOccurs="0"/>
    </xs:sequence>
  </xs:complexType>

  <xs:complexType name="etudiant">
    <xs:sequence>
      <xs:element name="email" type="xs:string" minOccurs="0"/>
      <xs:element name="nom" type="xs:string" minOccurs="0"/>
      <xs:element name="prenom" type="xs:string" minOccurs="0"/>
    </xs:sequence>
  </xs:complexType>

  <xs:complexType name="InfoAll">
    <xs:sequence/>
  </xs:complexType>

  <xs:complexType name="InfoAllResponse">
    <xs:sequence>
      <xs:element name="return" type="tns:etudiant" minOccurs="0" maxOccurs="unbounded"/>
    </xs:sequence>
  </xs:complexType>

  <xs:complexType name="ImportEtu">
    <xs:sequence/>
  </xs:complexType>

  <xs:complexType name="SuprEtudiant">
    <xs:sequence>
      <xs:element name="id" type="xs:int"/>
    </xs:sequence>
  </xs:complexType>

  <xs:complexType name="ModifieEtudiant">
    <xs:sequence>
      <xs:element name="attribueModifie" type="xs:string" minOccurs="0"/>
      <xs:element name="nouvelleValeur" type="xs:string" minOccurs="0"/>
      <xs:element name="idEtudiant" type="xs:int"/>
    </xs:sequence>
  </xs:complexType>

  <xs:complexType name="ExportEtu">
    <xs:sequence/>
  </xs:complexType>

  <xs:complexType name="GetEtuId">
    <xs:sequence>
      <xs:element name="nom" type="xs:string" minOccurs="0"/>
      <xs:element name="prenom" type="xs:string" minOccurs="0"/>
      <xs:element name="email" type="xs:string" minOccurs="0"/>
    </xs:sequence>
  </xs:complexType>

  <xs:complexType name="GetEtuIdResponse">
    <xs:sequence>
      <xs:element name="return" type="xs:int"/>
    </xs:sequence>
  </xs:complexType>

  <xs:complexType name="AjoutEtudiant">
    <xs:sequence>
      <xs:element name="nom" type="xs:string" minOccurs="0"/>
      <xs:element name="prenom" type="xs:string" minOccurs="0"/>
      <xs:element name="email" type="xs:string" minOccurs="0"/>
    </xs:sequence>
  </xs:complexType>
</xs:schema>

